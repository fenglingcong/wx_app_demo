page{
	height: 100%;
}
.container-box{
	background-color: #efeff4;
	height: 100%;
}
.img-box{
	position: relative;
	padding: 32rpx;
	background-color: #fff;
	border-bottom: 1rpx solid #e5e5e5;
}
.img-box image{
	width: 686rpx;
	height: 686rpx;
	background-color: ##f9f9f9;
}
.input-row{
	margin: 30rpx auto;
	padding: 0 32rpx;
	border-bottom: 1rpx solid #e5e5e5;
	border-top: 1rpx solid #e5e5e5;
	display: flex;
	align-items: center;
	height: 88rpx;
	background-color: #fff;
	font-size: 32rpx;
	color: #000;
}
.input-row input{
	margin-left: 100rpx;
	flex: 1;
}
.mybtn{
	width: 686rpx;
	margin: 60rpx auto;
}
.mask{
	position: fixed;
	top: 0;
	left: 0;
	z-index: 10;
	width: 100%;
	height: 100%;
	background-color: rgba(0, 0, 0, .2);
}
.canvas-box{
	position: fixed;
	top: -20000px;
	left: -30000px;
}